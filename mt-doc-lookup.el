(require 'cl-lib)

(defvar mt-doc-lookup-browser-function #'eww-browse-url
  "The browse-url-browser-function for browsing the Movable Type Documentation.")

(defvar mt-doc-lookup-base-url "http://www.movabletype.jp/documentation/appendices/"
  "The Movable Type documentation url.")

(defvar mt-doc-lookup-tags-base-url
  (concat mt-doc-lookup-base-url "tags/")
  "The Movable Type tags documentation url.")

(defvar mt-doc-lookup-config-directives-base-url
  (concat mt-doc-lookup-base-url "config-directives/")
  "The Movable Type config directives documentation url.")

(defvar mt-doc-lookup-modifiers-base-url
  (concat mt-doc-lookup-base-url "modifiers/")
  "The Movable Type modifiers documentation url.")

(defvar mt-doc-lookup-tags-hash (make-hash-table :test #'equal)
  "A lookup table of The Movable Type tags documentation.")
