(defvar mt-doc-lookup-all-list
  (cl-loop for h in (list mt-doc-lookup-tags-hash
                          mt-doc-lookup-config-directives-hash
                          mt-doc-lookup-modifiers-hash)
           append (cl-loop for k being the hash-keys in h
                           collect (list k (gethash k h))))
  "A lookup table of The Movable Type documentation.")


(defmacro mt-doc-lookup--def-lookup-command (name docstring prompt lookup-table)
  `(defun ,(intern (concat "mt-doc-lookup" (if name (format "-%s" name)))) (kwd)
  ,docstring
  (interactive
   (list
    (let ((completion-ignore-case t))
      (completing-read
       ,prompt
       ,lookup-table
       nil
       t))))
  (let ((browse-url-browser-function mt-doc-lookup-browser-function))
    (browse-url
     (if (hash-table-p ,lookup-table)
       (gethash kwd ,lookup-table)
       (cl-loop for pair in ,lookup-table
                when (equal (car pair) kwd)
                return (cadr pair)))))))

